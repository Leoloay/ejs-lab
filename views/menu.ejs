<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Menu</title>
  </head>
  <%- include('./partials/nav') %>
  <body>
    <h1>Full Menu</h1>
    <p>
      <% RESTAURANT.menu.forEach((dish, index) => { %>
      <li>
        Name: <%= dish.name %> | Price: <%= dish.price %> | Rating: <%=
        dish.rating %> | Details: <%= dish.details %>
      </li>
      <% }) %>
    </p>
  </body>
</html>
